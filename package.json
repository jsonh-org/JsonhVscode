{
  "name": "jsonh-vscode",
  "displayName": "JsonhVscode",
  "description": "Syntax highlighter for JSONH in Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jsonh",
      "aliases": ["JSONH", "jsonh"],
      "extensions": [".jsonh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jsonh",
      "scopeName": "source.jsonh",
      "path": "./syntaxes/jsonh.tmLanguage.json"
    }]
  }
}
